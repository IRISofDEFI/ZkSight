# Environment
ENVIRONMENT=development
LOG_LEVEL=INFO

# RabbitMQ
RABBITMQ__HOST=localhost
RABBITMQ__PORT=5672
RABBITMQ__USERNAME=guest
RABBITMQ__PASSWORD=guest
RABBITMQ__VHOST=/

# InfluxDB
INFLUXDB__URL=http://localhost:8086
INFLUXDB__TOKEN=dev-token-change-in-production
INFLUXDB__ORG=chimera
INFLUXDB__BUCKET=zcash_metrics

# MongoDB
MONGODB__URI=mongodb://admin:adminpassword@localhost:27017
MONGODB__DATABASE=chimera

# Redis
REDIS__HOST=localhost
REDIS__PORT=6379
REDIS__PASSWORD=
REDIS__DB=0

# OpenAI
OPENAI__API_KEY=your-api-key-here
